# Configurazione Hisense H-NET Decoder
# Copia questo file e rinominalo in .env

# Configurazione MQTT Broker
MQTT_BROKER=localhost
MQTT_PORT=1883
MQTT_TOPIC=hisense/hnet/raw
MQTT_USER=
MQTT_PASSWORD=

# Configurazione pubblicazione
PUBLISH_PREFIX=PDC

# Configurazione logging
LOG_LEVEL=INFO

# Configurazione Docker
COMPOSE_PROJECT_NAME=hisense-hnet
